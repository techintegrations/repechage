    "rich_text_iX7ygA": {
      "type": "rich-text",
      "blocks": {
        "heading_BqadNe": {
          "type": "heading",
          "settings": {
            "title": "THE AGENDA",
            "title_color": "#000000"
          }
        },
        "text_7q9VX6": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<h3><span style=\"text-decoration:underline\"><strong>Sunday, December 1, 2024 – Welcome Reception</strong></span></h3><p><strong>5:30pm-7:30pm</strong> – Join us for welcome cocktails and networking at the hotel skydeck overlooking the atlantic ocean 6th Floor Conrad Ft Lauderdale Beach</p>",
            "text_color": "#000000"
          }
        },
        "text_y73nyR": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<h3><span style=\"text-decoration:underline\"><strong>Monday December 2, 2024 – Day of Empowerment</strong></span></h3><ul><li><strong>8:30 - 9:30am:</strong> Conference Registration and enjoy Morning Light Bites and Coffee / Tea</li><li><strong>9:30am</strong>: Welcome To Day of Empowerment</li><li><strong>9:45am</strong>: Keynote Address by Girlie Joseph</li><li><strong>10:15 - 11:45am:</strong> Success at Every Level Panel Discussion</li><li><strong>11:45am - 12pm:</strong> 15min Break</li><li><strong>12 - 1pm:</strong> Presentation by Lydia Sarfati</li><li><strong>1 - 3pm:</strong> Lunch and Wellness Activation</li><li><strong>3 - 4pm: </strong>- Ronit Enos \"Profit First For Salons\"</li><li><strong>4 - 4:45pm:</strong> Edyta Satchell Presentation on Travel Wellness</li><li><strong>4:45 - 5pm:</strong> Closing Remarks</li></ul>",
            "text_color": "#000000"
          }
        },
        "text_iT3tr4": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<h3><span style=\"text-decoration:underline\"><strong>Tuesday December 3, 2024 Advanced Workshops</strong></span></h3><ul><li><strong>9 - 10am:</strong> Conference Registration and enjoy Light Bites and Coffee / Tea</li><li><strong>10am - 1pm:</strong> Advanced Retail Workshop with Lydia Sarfati and Ronit Enos</li><li><strong>1 - 2pm:</strong> Lunch and Networking</li><li><strong>2 - 5pm:</strong> Advanced Skincare Facial Massage Workshop endorsed by <strong>CIDESCO</strong></li></ul>",
            "text_color": "#000000"
          }
        }
      },
      "block_order": [
        "heading_BqadNe",
